<div class='container'>
    <div class="card">
        <div class="card-body">

            {{#each articles}}

            <a id='viewArticleButton' href='article-comments?id={{_id}}'><h5 id='articleHeadline' class="card-title">{{title}}</h5></a>

            <p id='articleSummary' class="card-text">{{description}}</p>

            {{/each}}

            
        </div>
    </div>
</div>
